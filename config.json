{
  "namespaces": {
    "include": [
      "inbox.edge_extra",
      "inbox.stix",
      "inbox.activity.log",
      "inbox.certuk_system_audit",
      "inbox.crashlog"
    ],
    "mapping": {
      "inbox.activity.log": "activity.activity.log",
      "inbox.certuk_system_audit": "system.certuk_system_audit",
      "inbox.crashlog": "error.crashlog"
    }
  },
  "docManagers": [
    {
      "docManager": "elastic2_doc_manager",
      "targetURL": "http://elasticsearch:9200",
      "args": {
        "clientOptions": {
          "timeout": 30,
          "use_ssl": true,
          "http_auth": "mongoconnector:mongoconnector"
        }
      }
    }
  ],
  "logging": {
    "type": "file",
    "filename": "/var/log/mongo-connector/mongo-connector.log"
  },
  "oplogFile": "/var/log/mongo-connector/mongo-connector.oplog",
  "mainAddress": "mongo:27017",
  "verbosity": 2,
  "continueOnError": true,
  "regex_exclude_fields": [
    ".*:.*",
    "data_.*"
  ],
  "exclude_fields": [

  ],
  "__fields": [
  ]
}
